
*{
	margin:0;
	padding:0;
}


html, body{
	width:100%;
	height:100%;
	background-image:url(../office.png);
	background-repeat:no-repeat;
	background-size:cover;
	background-attachment:scroll;
	padding-bottom:80px;
	/* background-position: 500px 200px; */
}

.contenedor{
	 width: 94%;
	 height:500px;
	 background-color:rgba(255,0,0,.5);
	 position:absolute;
	 top:10px;
	 left:10px;
}

.nav_p{
	position: relative;
    top: 55px;
    left: 31px;
    width: 94%;
	height:175px;
	background-color:rgba(0,255,0,.5);
	display:flex;
	justify-content:flex-start;
	align-items:center;
	border-top-left-radius:48px;
	border-top-right-radius:48px;
}

.nav_p li{
	list-style:none;
	/* margin-left:78px; */
	font-family:sans-serif;
	font-weight:600;
	font-size:13px;
	color:#163664;
}

.nav_p .icon_p{
	margin-left:475px;
	font-size:30px;
}

.nav_p .logo{
	margin-left:78px;
}

.nav_p .about{
	margin-left:70px;
}

.nav_p .contact{
	margin-left:71px;
}








