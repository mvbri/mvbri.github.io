
*{
	margin:0;
	padding:0;
}


html, body{
	width:100%;
	height:100%;
	background-image:url(../office.png);
	background-repeat:no-repeat;
	background-size:cover;
	background-attachment:scroll;
	padding-bottom:80px;
	/* background-position: 500px 200px; */
}

/*CONTENEDOR PRINCIPAL*/

.contenedor{
	 width: 94%;
	 height:650px;
	 background-color:rgba(255,0,0,.5);
	 position:relative;
	 top:54px;
	 left:26px;
	 border-top-left-radius:48px;
     border-top-right-radius:48px;
	 border-bottom-left-radius:48px;
	 border-bottom-right-radius:48px;
}

/*FIN DEL CONTENEDOR PRINCIPAL*/

/*-----------------------*/

/*HEADER*/

.nav_p{
	position: relative;
    top: 0px;
    left: 0px;
    width: 100%;
	height:177px;
	background-color:rgba(0,255,0,.5);
	display:flex;
	justify-content:flex-start;
	align-items:center;
	border-top-left-radius:48px;
	border-top-right-radius:48px;
	/* flex-wrap:wrap; */
}

.nav_p li{
	list-style:none;
	/* margin-left:78px; */
	font-family:sans-serif;
	font-weight:600;
	font-size:13px;
	color:#163664;
}

.nav_p .icon_p{
	margin-left:475px;
	font-size:30px;
}

.nav_p .logo{
	margin-left:83px;
}

.nav_p .about{
	margin-left:70px;
}

.nav_p .contact{
	margin-left:71px;
}

/*FIN DEL HEADER*/

/*--------------*/

/*SECTION PRINCIPAL*/

.seccion_p{
	display:flex;
}

.enunciado_p{
	
	width:50%;
}

.enunciado_p h1{
	font-size:56px;
	color:purple;
	padding-left:73px;
	font-weight:900;
}

.enunciado_p p{
	font-size:11px;
	color:lightgray;
	padding-left:76px;
	padding-right:0px;
	padding-top:26px;
}

.boton_p{
	background:purple;
	color:#FFF;
	font-weight:bold;
	border-style:none;
	padding:6px 25px;
	border-radius:25px;
	margin-top:15px;
	margin-left:80px;
}

.img_p{
	z-index:1000;
	justify-content:space-evenly;
}

.img_p img{
	width:540px;
	margin-right:16px;
	margin-top:6px;
}

footer{
	width:100%;
}

footer p{
	font-size:12px;
	padding-left:77px;
	padding-top:25px;
	font-weight:600;
}









