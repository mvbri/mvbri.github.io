
*{
	margin:0;
	padding:0;
}

@font-face{
	font-family:sans;
	src:url(fonts/SourceSansPro-Regular.otf);
}

@font-face{
	font-family:sans-2;
	src:url(fonts/SourceSansPro-Bold.otf);
}

@font-face{
	font-family:sans-3;
	src:url(fonts/SourceSansPro-LightIt.otf);
}


html, body{
	/* width:100%; */
	/* height:100%; */
	/* background-image:url(../office.png); */
	/* background-repeat:no-repeat; */
	/* background-size:cover; */
	/* background-attachment:scroll; */
	/* padding-bottom:80px; */

}

.fondo{
	width:100%;
	height:100%;
	position:relative;
	background:linear-gradient(to right,#e5e5e5 75%,#dbdcde 25%);
	border-radius:15px;
}

/*CONTENEDOR PRINCIPAL*/

.contenedor{
	background:#FFF;
	 width: 94%;
	 height:650px;
	 position:relative;
	 top:54px;
	 left:26px;
	 border-top-left-radius:48px;
     border-top-right-radius:48px;
	 border-bottom-left-radius:48px;
	 border-bottom-right-radius:48px;
	 /* border: 10px solid #efefef; */
	 border: 10px solid rgba(242, 244, 244,.5) ;
	 background-clip:padding-box;
	 box-sizing: content-box;
	 margin-bottom:25px;
}

/*FIN DEL CONTENEDOR PRINCIPAL*/

/*-----------------------*/

/*HEADER*/

.nav_p{
	position: relative;
    top: 0px;
    left: 0px;
    width: 100%;
	height:172px;
	display:flex;
	justify-content:flex-start;
	align-items:center;
	border-top-left-radius:48px;
	border-top-right-radius:48px;
	/* flex-wrap:wrap; */
}

.nav_p li{
	list-style:none;
	/* margin-left:78px; */
	font-family:sans-serif;
	font-weight:600;
	font-size:13px;
	color:#576d90;
}

.nav_p .icon_p{
	margin-left:472px;
	font-size:32px;
}

.nav_p .logo{
	margin-left:81px;
}

.nav_p .about{
	margin-left:70px;
}

.nav_p .contact{
	margin-left:70px;
}

/*FIN DEL HEADER*/

/*--------------*/

/*SECTION PRINCIPAL*/

.seccion_p{
	display:flex;
}

.enunciado_p{
	
	width:50%;
}

.enunciado_p h1{
	font-size:56px;
	color:#5d66ff;
	padding-left:73px;
	font-weight:500;
	font-family:sans;
}

.enunciado_p p{
	font-size:11px;
	color:#c7c8c9;
	padding-left:76px;
	padding-right:0px;
	padding-top:26px;
	/* font-weight:600; */
	font-family:sans-3;
}

.boton_p{
	background:#5d66ff;
	color:#FFF;
	font-size:14px;
	font-family:sans-2;
	/* font-weight:200; */
	border-style:none;
	padding:4px 25px;
	border-radius:25px;
	margin-top:30px;
	margin-left:80px;
}

.img_p{
	z-index:1000;
	justify-content:space-evenly;
}

.img_p img{
	width:540px;
	margin-right:23px;
	margin-top:8px;
}



footer p{
	font-size:13px;
	padding-left:77px;
	padding-top:15px;
	font-weight:600;
	color:#d2d3d5;
}









